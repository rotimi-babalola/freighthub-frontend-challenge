{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint:latest",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": false,
        "arrow-parens": false,
        "member-access": [true, "no-public"],
        "jsx-no-multiline-js": false,
        "object-literal-sort-keys": false,
        "arrow-return-shorthand": true,
        "semicolon": false,
        "jsx-no-bind": false,
        "jsx-no-lambda": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts",
            "coverage/lcov-report/*.js",
            "webpack.common.js"
        ]
    }
}
